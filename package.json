{
  "name": "snapi",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "clean": "rimraf dist",
    "reset": "npm run clean && npm ci",
    "dev": "concurrently \"npm run watch\" \"npm run server\"",
    "server": "nodemon dist/server.js",
    "watch": "tsc --watch",
    "start": "npm run build && node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node dist/seeds/index.js"
  },
  "keywords": [],
  "author": "Clint Jones",
  "license": "ISC",
  "description": "Read your friends' thoughts, add them to a list and give them your opinion using Express.js, MongoDB, Mongoose ODM.",
  "dependencies": {
    "express": "^4.21.2",
    "mongoose": "^8.10.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/mongoose": "^5.11.96",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  }
}
